        <ns4:ART>
          <rdf:Description>
            <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SECTION"/>
            <ns6:ECTNO>6301.0</ns6:ECTNO>
            <ns6:UBJECT>Foreword.</ns6:UBJECT>
            <ns4:>A Department of Transportation Board of Contract Appeals has been established pursuant to Pub. L. 95-563. The Secretary appoints the members of the Board and designates the Chair and Vice-Chair of the Board.</ns4:>
          </rdf:Description>
        </ns4:ART>

        <ns4:ART>
          <rdf:Description>
            <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SECTION"/>
            <ns6:ECTNO>6301.1</ns6:ECTNO>
            <ns6:UBJECT>Scope of part.</ns6:UBJECT>
            <ns4:>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/E"/>
              </rdf:Description>
            </ns4:>

