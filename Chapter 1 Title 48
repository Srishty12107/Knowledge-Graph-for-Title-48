            <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SECTION"/>
            <ns6:ECTNO>6302.19</ns6:ECTNO>
            <ns6:UBJECT>Interrogatories to parties, admission of facts, and inspection of documents (Rule 19).</ns6:UBJECT>
            <ns4:>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/E"/>
              </rdf:Description>
