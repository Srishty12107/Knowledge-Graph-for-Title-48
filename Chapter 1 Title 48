            <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/PART"/>
            <ns11:AR>Pt. 2942</ns11:AR>
            <ns9:D>PART 2942—CONTRACT ADMINISTRATION AND AUDIT SERVICES</ns9:D>
            <ns3:ONTENTS>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SUBPART"/>
                <ns9:D>Subpart 2942.1—Contract Audit Services</ns9:D>
                <ns6:ECHD>Sec.</ns6:ECHD>
                <ns6:ECTNO>2942.101</ns6:ECTNO>
                <ns6:UBJECT>Policy.</ns6:UBJECT>
              </rdf:Description>
            </ns3:ONTENTS>

            <ns3:ONTENTS>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SUBPART"/>
                <ns9:D>Subpart 2942.15—Contractor Performance Information</ns9:D>
                <ns6:ECTNO>2942.1501</ns6:ECTNO>
                <ns6:ECTNO>2942.1502</ns6:ECTNO>
                <ns6:ECTNO>2942.1503</ns6:ECTNO>
                <ns6:UBJECT>Scope.</ns6:UBJECT>
                <ns6:UBJECT>Policy.</ns6:UBJECT>
                <ns6:UBJECT>Procedures.</ns6:UBJECT>
              </rdf:Description>
            </ns3:ONTENTS>

            <ns1:UTH>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/HD"/>
              </rdf:Description>
            </ns1:UTH>

            <ns1:UTH>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/P"/>
