            <ns6:ECTNO>6302.18</ns6:ECTNO>
            <ns6:UBJECT>Discovery-depositions (Rule 18).</ns6:UBJECT>
            <ns4:>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/E"/>
