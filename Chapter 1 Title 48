            <ns9:D>PART 2903—IMPROPER BUSINESS PRACTICES AND PERSONAL CONFLICTS OF INTEREST</ns9:D>
            <ns3:ONTENTS>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SUBPART"/>
                <ns9:D>Subpart 2903.1—Safeguards</ns9:D>
                <ns6:ECHD>Sec.</ns6:ECHD>
                <ns6:ECTNO>2903.101</ns6:ECTNO>
                <ns6:ECTNO>2903.101-1</ns6:ECTNO>
                <ns6:ECTNO>2903.104</ns6:ECTNO>
