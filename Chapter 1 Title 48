                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SUBPART"/>
                <ns9:D>Subpart 2913.2—Actions At Or Below The Micro-Purchase Threshold</ns9:D>
                <ns6:ECTNO>2913.201</ns6:ECTNO>
                <ns6:UBJECT>General.</ns6:UBJECT>
              </rdf:Description>
            </ns3:ONTENTS>

            <ns3:ONTENTS>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SUBPART"/>
                <ns9:D>Subpart 2913.3—Simplified Acquisition Methods</ns9:D>
                <ns6:ECTNO>2913.301</ns6:ECTNO>
                <ns6:ECTNO>2913.307</ns6:ECTNO>
                <ns6:UBJECT>Governmentwide commercial purchase card.</ns6:UBJECT>
                <ns6:UBJECT>Forms.</ns6:UBJECT>
              </rdf:Description>
            </ns3:ONTENTS>

            <ns1:UTH>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/HD"/>
              </rdf:Description>
