              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SECTION"/>
                <ns6:ECTNO>3401.401</ns6:ECTNO>
                <ns6:UBJECT>Definition.</ns6:UBJECT>
                <ns4:>A deviation from the EDAR has the same meaning as a deviation from the FAR.</ns4:>
              </rdf:Description>
            </ns6:UBPART>

            <ns6:UBPART>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SECTION"/>
                <ns6:ECTNO>3401.403</ns6:ECTNO>
                <ns6:UBJECT>Individual deviations.</ns6:UBJECT>
                <ns4:>An individual deviation from the FAR or the EDAR must be approved by the Senior Procurement Executive (SPE).</ns4:>
              </rdf:Description>
            </ns6:UBPART>

            <ns6:UBPART>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SECTION"/>
                <ns6:ECTNO>3401.404</ns6:ECTNO>
                <ns6:UBJECT>Class deviations.</ns6:UBJECT>
                <ns4:>A class deviation from the FAR or the EDAR must be approved by the Chief Acquisition Officer (CAO).</ns4:>
              </rdf:Description>
            </ns6:UBPART>

