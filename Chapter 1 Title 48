            <ns3:ONTENTS>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/SUBPART"/>
                <ns9:D>Subpart 3037.1—Service Contracts—General</ns9:D>
                <ns6:ECHD>Sec.</ns6:ECHD>
                <ns6:ECTNO>3037.103</ns6:ECTNO>
                <ns6:ECTNO>3037.104</ns6:ECTNO>
                <ns6:ECTNO>3037.104-70</ns6:ECTNO>
                <ns6:ECTNO>3037.104-90</ns6:ECTNO>
                <ns6:ECTNO>3037.104-91</ns6:ECTNO>
                <ns6:UBJECT>[Reserved]</ns6:UBJECT>
                <ns6:UBJECT>Personal services contracts.</ns6:UBJECT>
                <ns6:UBJECT>Personal services contracts (USCG).</ns6:UBJECT>
                <ns6:UBJECT>Personal services with individuals under the authority of 10 U.S.C. 1091 (USCG).</ns6:UBJECT>
              </rdf:Description>
