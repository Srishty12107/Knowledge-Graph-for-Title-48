                <ns6:ECHD>Sec.</ns6:ECHD>
                <ns6:ECTNO>3047.305</ns6:ECTNO>
                <ns6:ECTNO>3047.305-70</ns6:ECTNO>
                <ns6:UBJECT>Solicitation provisions, contract clauses, and transportation factors.</ns6:UBJECT>
                <ns6:UBJECT>Solicitation provision.</ns6:UBJECT>
              </rdf:Description>
            </ns3:ONTENTS>

            <ns1:UTH>
              <rdf:Description>
                <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/HD"/>
