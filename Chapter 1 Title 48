            <ns6:ECHD>Sec.</ns6:ECHD>
            <ns6:ECTNO>5145.301</ns6:ECTNO>
            <ns6:ECTNO>5145.302-3</ns6:ECTNO>
            <ns6:ECTNO>5145.303</ns6:ECTNO>
            <ns6:UBJECT>Definitions.</ns6:UBJECT>
            <ns6:UBJECT>Other contracts.</ns6:UBJECT>
            <ns6:UBJECT>Providing material.</ns6:UBJECT>
          </rdf:Description>
        </ns4:ART>

        <ns4:ART>
          <rdf:Description>
            <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/AUTH"/>
            <ns9:D>Authority:</ns9:D>
            <ns4:>5 U.S.C. 301, 10 U.S.C. 2202, DoD Directive 5000.35, and DoD FAR Supplement 201.301.</ns4:>
          </rdf:Description>
