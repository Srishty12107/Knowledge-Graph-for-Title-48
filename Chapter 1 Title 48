                <ns6:ECTNO>3416.303</ns6:ECTNO>
                <ns6:UBJECT>Cost-sharing contracts.</ns6:UBJECT>
                <ns4:>
                  <rdf:Description>
                    <rdf:type rdf:resource="https://www.gpo.gov/fdsys/bulkdata/CFR/2015/title-48/E"/>
                  </rdf:Description>
